<Window x:Class="Client.ShowServicesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="Show Services Window" Height="450" Width="800">
    <Grid>
        <StackPanel x:Name="sp" HorizontalAlignment="Left" Height="337" VerticalAlignment="Top" Width="180" Margin="20,55,0,0"/>
        <Label x:Name="NameLabel" Content="All Services:" HorizontalAlignment="Left" Height="38" Margin="20,12,0,0" VerticalAlignment="Top" Width="180"/>
        <Button x:Name="BackButton" Content="Back" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="239,15,0,0" Click="BackButton_Click"/>
        <TextBlock x:Name="ErrorText" TextAlignment="Center" HorizontalAlignment="Left" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Margin="239,82,0,0"/>

    </Grid>
</Window>
